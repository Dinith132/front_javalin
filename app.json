{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8478ec05-2fa4-45e2-8fdc-210c7c7eab99"
      }
    },
    "android": {
      "package": "com.dinithp132.boltexponativewind"
    },
    "permissions": {
      "camera": {
        "ios": {
          "NSCameraUsageDescription": "This app uses the camera to record javelin throws for analysis.",
          "NSMicrophoneUsageDescription": "This app uses the microphone to record audio with video."
        },
        "android": {
          "permissions": ["android.permission.RECORD_AUDIO","android.permission.CAMERA" ]
        }
      }
    }
  }
}
